    "undici": "^7.2.0"
  },
  "devDependencies": {
    "@tsconfig/bases": "1.0.16",
    "@types/node": "24.10.1",
    "@types/promise-retry": "1.1.6",
    "eslint": "9.39.2",
    "eslint-config-universe": "15.0.3",

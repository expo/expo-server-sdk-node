  "dependencies": {
    "node-fetch": "^3.3.2",
    "promise-limit": "^2.7.0",
    "promise-retry": "^2.0.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "20.19.1",
    "@types/promise-retry": "1.1.6",
    "eslint": "9.26.0",
    "eslint-config-universe": "15.0.3",
    "jest": "30.0.3",
    "jiti": "2.4.2",
    "msw": "2.10.2",
    "prettier": "3.5.3",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  },
  "packageManager": "yarn@4.9.2"

    "promise-limit": "^2.7.0",
    "promise-retry": "^2.0.1"
  },
  "devDependencies": {
    "@tsconfig/node-ts": "23.6.2",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.17.2",
    "@types/node-fetch": "2.6.12",
    "@types/promise-retry": "1.1.6",
    "eslint": "9.33.0",
    "eslint-config-universe": "15.0.3",
    "expect": "30.2.0",
    "jiti": "2.4.2",
    "msw": "2.10.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2"

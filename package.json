{
  "name": "expo-server-sdk",
  "version": "4.0.0",
  "description": "Server-side library for working with Expo using Node.js",
  "type": "module",
  "main": "build/ExpoClient.js",
  "types": "build/ExpoClient.d.ts",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "build": "yarn prepack",

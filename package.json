    "test": "node --test --experimental-test-coverage --env-file .env",
    "tsc": "tsc",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/expo-server-sdk-node.git"
